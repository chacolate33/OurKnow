<%= form_with  url: group_knowledges_path, local:true do |f| %>
  <%= render 'layouts/errors', model: knowledge %>
  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_field :content, class: 'form-control' %>
  </div
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>
<div class="container">
  <%= form_for @user, url: users_path do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :mail %>
    <%= f.text_field :mail%>
    
    <%= f.label :password %>
    <%= f.text_field :password_confirmation %>
    
    <%= f.submit "登録" %>
  <% end %>
  
</div>